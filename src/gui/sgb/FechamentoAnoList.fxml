<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="288.0" prefWidth="698.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.sgb.FechamentoAnoListController">
   <children>
      <ToolBar blendMode="DIFFERENCE" prefHeight="40.0" prefWidth="560.0">
        <items>
            <Label fx:id="labelTitulo" prefHeight="22.0" prefWidth="393.0">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <padding>
                  <Insets bottom="2.0" left="100.0" top="2.0" />
               </padding>
            </Label>
            <Label fx:id="labelUser" prefHeight="15.0" prefWidth="107.0" text=" " textFill="#80dddd">
               <font>
                  <Font size="12.0" />
               </font>
               <padding>
                  <Insets left="15.0" />
               </padding>
            </Label>
        </items>
         <effect>
            <Blend />
         </effect>
      </ToolBar>
      <TableView fx:id="tableViewAnual" blendMode="DARKEN" prefHeight="235.0" prefWidth="674.0">
        <columns>
            <TableColumn fx:id="tableColumnCartelaFechamentoAno" editable="false" prefWidth="47.0" text="Cartela" />
            <TableColumn fx:id="tableColumnDataFechamentoAno" editable="false" prefWidth="72.0" text="Data" />
            <TableColumn fx:id="tableColumnSituacaoFechamentoAno" editable="false" prefWidth="55.0" text="Situação" />
            <TableColumn fx:id="tableColumnValorCartelaFechamentoAno" editable="false" minWidth="0.0" prefWidth="105.0" text="Vlr Cartela" />
            <TableColumn fx:id="tableColumnValorProdutoFechamentoAno" editable="false" prefWidth="76.0" text="- Produto" />
            <TableColumn fx:id="tableColumnValorComissaoFechamentoAno" editable="false" prefWidth="89.0" text="- Comissão" />
            <TableColumn fx:id="tableColumnValorResultadoFechamentoAno" editable="false" prefWidth="103.0" text="= Resultado +" />
            <TableColumn fx:id="tableColumnValorAcumuladoFechamentoAno" editable="false" prefWidth="144.0" text="= Acumulado (Receita)" />
        </columns>
         <effect>
            <Blend />
         </effect>
      </TableView>
   </children>
</VBox>
